<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

<record id="module_category_imas" model="ir.module.category">
    <field name="name">i.MAS</field>
    <field name="sequence">30</field>
</record>

<record id="group_imas_user" model="res.groups">
    <field name="name">User</field>
    <field name="category_id" ref="module_category_imas"/>
    <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
</record>

<record id="group_imas_manager" model="res.groups">
    <field name="name">Manager</field>
    <field name="category_id" ref="module_category_imas"/>
    <field name="implied_ids" eval="[(4, ref('group_imas_user'))]"/>
    <field name="users" eval="[(4, ref('base.user_root'))]"/>
</record>

</odoo>
